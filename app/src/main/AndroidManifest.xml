<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booklists">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookLists">
        <activity
            android:name=".WebActivity"
            android:exported="true" />
        <activity
            android:name=".CurrentlyReadingBooks"
            android:label="@string/currently_reading"
            android:exported="true" />
        <activity
            android:name=".FavoriteBooks"
            android:label="@string/favorite_books"
            android:exported="true" />
        <activity
            android:name=".WantToReadBooks"
            android:label="@string/want_to_read"
            android:exported="true" />
        <activity
            android:name=".AlreadyReadBooks"
            android:label="@string/already_read"
            android:exported="true" />
        <activity
            android:name=".BookActivity"
            android:label="@string/detail_book"
            android:exported="true" />
        <activity
            android:name=".BookList"
            android:label="@string/all_book_list"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>